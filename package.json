{
  "name": "fastcombinators",
  "version": "1.0.0",
  "description": "High-performance functional combinator library for JavaScript/TypeScript based on Cursor-Sink pattern",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "demo": "ts-node --project tsconfig.node.json examples/basic-usage.ts",
    "demo:helpers": "ts-node --project tsconfig.node.json examples/helpers-usage.ts",
    "bench": "node --expose-gc node_modules/ts-node/dist/bin.js --project tsconfig.node.json benchmarks/main.ts",
    "bench:list": "node --expose-gc node_modules/ts-node/dist/bin.js --project tsconfig.node.json benchmarks/main.ts --list",
    "bench:help": "node --expose-gc node_modules/ts-node/dist/bin.js --project tsconfig.node.json benchmarks/main.ts --help"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lameach/fastcombinators.git"
  },
  "keywords": [
    "functional-programming",
    "combinators",
    "transducers",
    "cursor-sink",
    "high-performance",
    "zero-allocation",
    "stream-processing",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lameach/fastcombinators/issues"
  },
  "homepage": "https://github.com/Lameach/fastcombinators#readme",
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "mitata": "^0.1.14",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "ramda": "^0.30.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
